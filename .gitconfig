[commit]
    gpgsign = true

[diff]
	algorithm = histogram

[include]
    path = .gitprofile
    path = .gitaliases

[core]
	autocrlf = input
	safecrlf = false
	pager = less -FRX --mouse
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
